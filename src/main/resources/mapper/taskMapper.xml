<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.test.dao.TaskMapper">
    <resultMap id="task" type="com.example.test.pojo.Task">
        <id column="uuid" property="uuid" javaType="String" jdbcType="VARCHAR"/>
        <result column="id" property="id" javaType="Integer" jdbcType="INTEGER"/>
        <result column="name" property="name" javaType="String" jdbcType="VARCHAR"/>
        <result column="lunch_time" property="lunchTime" javaType="Long" jdbcType="BIGINT"/>
    </resultMap>
    <select id="getTaskNeededDeleted" resultType="String" parameterType="Long">
        select path from taskevent where lunch_time <![CDATA[<=]]> #{lunchTime}
    </select>
    <select id="getPath" resultType="String" parameterType="String">
        select path from taskevent where uuid = #{uuid}
    </select>
    <select id="getName" parameterType="String" resultType="String">
        select name from taskevent where uuid = #{uuid}
    </select>
    <select id="getId" resultType="Integer" parameterType="String">
        select id from taskevent where uuid = #{uuid}
    </select>
</mapper>